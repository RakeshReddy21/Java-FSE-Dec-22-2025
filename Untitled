> var arr1=[]
undefined
> typeof(arr1)
'object'
> arr1[0] = 43;
43
> arr1[1] = 46;
46
> arr1[2] = 49;
49
> arr1
[ 43, 46, 49 ]
> arr1.length
3
> arr1[4] = [1,2,3];
[ 1, 2, 3 ]
> arr1
[ 43, 46, 49, <1 empty item>, [ 1, 2, 3 ] ]
> arr1[3] = 'Hai'
'Hai'
> arr1
[ 43, 46, 49, 'Hai', [ 1, 2, 3 ] ]
> arr1[5] = {name:'Alice', age:12}
{ name: 'Alice', age: 12 }
> arr1
