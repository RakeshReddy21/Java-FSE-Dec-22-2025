> const a = 10;
undefined
> const a = 10;
Uncaught SyntaxError: Identifier 'a' has already been declared
> typeof(a)
'number'
> x=[1,2,3]
[ 1, 2, 3 ]
> x=[1,2,3]
[ 1, 2, 3 ]
> x=[1,2,3];
[ 1, 2, 3 ]
> x=[1,2,3];
[ 1, 2, 3 ]
> x=[1,2,3]
[ 1, 2, 3 ]
> const x=[1,2,3]
undefined
> stu = {name:'manish', mark:72}
{ name: 'manish', mark: 72 }
> stu.mark=100;
100
> stu.age=12;
12
> stu
{ name: 'manish', mark: 100, age: 12 }
----------------------------------------

> var a = null;
undefined
> var b = undefined;
undefined
> a == b;
true
> a === b;
false
> a !== null
false
> b !== null
true
> var onj1 = {};
undefined
> var obj1 = {};
undefined
> typeof(obj1)
'object'
> obj1.x=10;
10
> obj1.y=20;
20
> obj1
{ x: 10, y: 20 }
> Object.keys
[Function: keys]
> Object.keys(obj1);
[ 'x', 'y' ]
> Object.values(obj1);
[ 10, 20 ]

> obj1.z=78;
78
> obj1
{ x: 10, y: 20, z: 78 }
> let str = {name: 'manish', age : 12}
undefined
> str
{ name: 'manish', age: 12 }
> str.name
'manish'
> str['marks']=67
67
> str
{ name: 'manish', age: 12, marks: 67 }
> str.['city']='Hyderabad'
str.['city']='Hyderabad'
    ^

Uncaught SyntaxError: Unexpected token '['
> str.city='Hyderabad'
'Hyderabad'
> str
{ name: 'manish', age: 12, marks: 67, city: 'Hyderabad' }
>
