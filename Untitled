> var robot1={
| name:'chitti'
| model:'GH67'
model:'GH67'
^^^^^

Uncaught SyntaxError: Unexpected identifier 'model'
> var robot1={
| name:'chitti'
| model:'GH67'}
model:'GH67'}
^^^^^

Uncaught SyntaxError: Unexpected identifier 'model'
> var robot1={
| name:'chitti',
| model:'GH67'}
undefined
> robot1
{ name: 'chitti', model: 'GH67' }
> var rb1 = Object.freeze(robot1);
undefined
> rb1
{ name: 'chitti', model: 'GH67' }
> robot1
{ name: 'chitti', model: 'GH67' }
> rb1.weight=567;
567
> rb1
{ name: 'chitti', model: 'GH67' }
> robot1.weight=567;
567
> var obj1 = {x:10, y:20}
undefined
> var obj2 = {x:10, y:25, z:30}
undefined
> var res = Object.assign(obj1, obj2);
undefined
> obj1;
{ x: 10, y: 25, z: 30 }
> obj2;
{ x: 10, y: 25, z: 30 }
> res
{ x: 10, y: 25, z: 30 }
> obj1
{ x: 10, y: 25, z: 30 }
> obj4={k:89};
{ k: 89 }
> var res = Object.assign(obj1.obj4);
Uncaught TypeError: Cannot convert undefined or null to object
    at Object.assign (<anonymous>)
> var res = Object.assign(obj1,obj4);
undefined
> obj1
{ x: 10, y: 25, z: 30, k: 89 }

